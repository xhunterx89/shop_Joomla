			<field name="" type="spacer" default="" label="" description="" />
			<field name="" type="spacer" default="" label="" description="" />



		
			<field
					name="module_width" 
					type="text"
					default="725"
					label="MODULE_WIDTH"
					description="MODULE_WIDTH_DESC">
				</field>	
				
				<field 
					name="module_height"
					type="text" 
					default="360" 
					label="MODULE_HEIGHT" 
					description="MODULE_HEIGHT_DESC">
				</field>
				
				<field name="" type="spacer" default="" label="" description="" />
					<field name="" type="spacer" default="" label="" description="" />
			
			<field 
